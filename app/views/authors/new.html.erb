<%= form_for @author, url: {action: "create"} do |f| %>

    <h4> First name </h4>
    <%= f.text_field :First_name %>
    <h4> Last name </h4>
    <%= f.text_field :Last_name %>
    <h4> Nickname </h4>
    <%= f.text_field :nickname %>
    <h4> HomeTown </h4>
    <%= f.text_field :hometown %>
    <h4> Age </h4>
    <%= f.number_field :age%>
    
    <%= f.submit "Save" %>
<% end %>